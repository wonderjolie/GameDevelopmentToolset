def EmptyDir(a_dir):
    for file in os.listdir(a_dir):
        file_path = os.path.join(a_dir, file)
        try:
            if os.path.isfile(file_path):
                os.unlink(file_path)
            elif os.path.isdir(file_path):
                shutil.rmtree(file_path)
        except:
            pass

node = hou.pwd()
geo = node.geometry()

bin = geo.attribValue(\"bin\")
cache = geo.attribValue(\"cache\")

EmptyDir(os.path.join(cache, \"12_Texturing\"))

func = \"aliceVision_texturing.exe\"

cmd = os.path.join(bin, func) 
cmd = [cmd]

cmd.append(\"--ini\")
ini = os.path.join(cache, \"06_PrepareDenseScene\", \"mvs.ini\")
cmd.append(ini)

cmd.append(\"--inputDenseReconstruction\")
recon = os.path.join(cache, \"10_Meshing\", \"denseReconstruction.bin\")
cmd.append(recon)

cmd.append(\"--inputMesh\")
input = os.path.join(cache, \"11_MeshFiltering\", \"mesh.obj\")
cmd.append(input)

cmd.append(\"--textureSide\")
side = node.parent().parm(\"side\").eval()
cmd.append(\"%s\" % (side))

cmd.append(\"--downscale\")
down = node.parent().parm(\"down\").evalAsInt()
cmd.append(\"%s\" % down)

cmd.append(\"--outputTextureFileType\")
file = node.parent().parm(\"file\").eval()
cmd.append(file)

cmd.append(\"--unwrapMethod\")
unwrap = node.parent().parm(\"unwrap\").eval()
cmd.append(unwrap)

