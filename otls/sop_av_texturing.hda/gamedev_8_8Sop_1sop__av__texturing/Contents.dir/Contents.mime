cmd.append(\"--flipNormals\")
flipN = node.parent().parm(\"flipN\").eval()
cmd.append(\"%s\" % (flipN))

cmd.append(\"--padding\")
pad = node.parent().parm(\"pad\").evalAsInt()
cmd.append(\"%s\" % (pad))

cmd.append(\"--maxNbImagesForFusion\")
maxImg = node.parent().parm(\"maxImg\").eval()
