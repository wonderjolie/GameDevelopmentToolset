snippet	[ 0	locks=0 ]	(	"int sh = primhedge(0, @primnum);

int h = sh;
do
{
    // Find partner prim (may be self)
    int oprim = hedge_prim(0, hedge_nextequiv(0, h));
    // See if it is same side and lower isle.
    int oside = prim(0, 'side', oprim);
    int oisle = prim(0, 'isle', oprim);
    if ((oside == i@side || i@side == -1) )//&& oisle < i@isle)
    {
        @isle = oisle;
        if (i@side == -1)
            i@side = oside;
    }
    h = hedge_next(0, h);
} while (h != sh && h != -1);
"	)
