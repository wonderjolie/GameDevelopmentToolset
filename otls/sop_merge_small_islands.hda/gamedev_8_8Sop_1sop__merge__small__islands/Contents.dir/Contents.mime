snippet	[ 0	locks=0 ]	(	"int npr = nprimitives(0);
int test[];
for(int i = 0; i <= npr; i++) 
{
        int oisle = prim(0, 'isle', i);
        push(test, oisle);
}

for(int i = 0; i <= npr; i++) 
{

    int sh = primhedge(0, i);

    i[]@islands = {};

    int h = sh;
    do
    {
        int oprim = hedge_prim(0, hedge_nextequiv(0, h));
        int oisle = test[oprim];//prim(0, 'isle', oprim);
        push(i[]@islands, oisle);
        h = hedge_next(0, h);
    
    } while (h != sh && h != -1);
   
    int hits[] = find(i[]@islands, prim(0, \"isle\", i));
    int match = 0;
    if (len(hits) == 1)
    {
        foreach (int num; @islands) 
        {
            int ohits[] = find(@islands,num);
            if (len(ohits) == 2)
            {
                setprimattrib(0, \"isle\", i, num, \"set\");
                test[i] = num;

                break;
            } 
        } 
    }

}


"	)
