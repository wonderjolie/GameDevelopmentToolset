--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/09_Meshing.parm"
Content-Type: text/plain

{
version 0.8
python	[ 0	locks=0 ]	(	"import hou, subprocess, os, gamedevutils

node = hou.pwd()
geo = node.geometry()

bin = geo.attribValue(\"bin\")
cache = geo.attribValue(\"cache\")

gamedevutils.empty_directory_recursive(os.path.join(cache, \"09_Meshing\"))

func = \"aliceVision_meshing\"

cmd = os.path.join(bin, func) 
cmd = [cmd]

######

cmd.append(\"--input\")
cmd.append(os.path.join(cache, \"05_StructureFromMotion\", \"sfm.abc\"))

cmd.append(\"--output\")
cmd.append(os.path.join(cache, \"09_Meshing\", \"sfm.abc\"))

cmd.append(\"--outputMesh\")
cmd.append(os.path.join(cache, \"09_Meshing\", \"mesh.obj\"))

cmd.append(\"--depthMapsFolder\")
cmd.append(os.path.join(cache, \"07_DepthMap\"))

cmd.append(\"--depthMapsFilterFolder\")
cmd.append(os.path.join(cache, \"08_DepthMapFilter\"))

cmd.append(\"--maxInputPoints\")
cmd.append(\"%s\" % node.parent().parm(\"maxInpt\").evalAsInt())

cmd.append(\"--maxPoints\")
cmd.append(\"%s\" % node.parent().parm(\"maxPts\").evalAsInt())

cmd.append(\"--maxPointsPerVoxel\")
cmd.append(\"%s\" % node.parent().parm(\"maxPtsVox\").evalAsInt())

cmd.append(\"--minStep\")
cmd.append(\"%s\" % node.parent().parm(\"minStep\").evalAsInt())

cmd.append(\"--simFactor\")
cmd.append(\"%s\" % node.parent().parm(\"sim\").evalAsInt())

cmd.append(\"--angleFactor\")
cmd.append(\"%s\" % node.parent().parm(\"angle\").evalAsInt())

cmd.append(\"--partitioning\")
cmd.append(node.parent().parm(\"part\").evalAsString())

cmd.append(\"--repartition\")
cmd.append(node.parent().parm(\"rePart\").evalAsString())

cmd.append(\"--estimateSpaceFromSfM\")
cmd.append(\"%s\" % node.parent().parm(\"estimateSpaceFromSfM\").evalAsInt())

cmd.append(\"--addLandmarksToTheDensePointCloud\")
cmd.append(\"%s\" % node.parent().parm(\"addLandmarksToTheDensePointCloud\").evalAsInt())

####
cmd.append(\"--colorizeOutput\")
cmd.append(\"%s\" % node.parent().parm(\"colorizeOutput\").evalAsInt())

cmd.append(\"--estimateSpaceMinObservations\")
cmd.append(\"%s\" % node.parent().parm(\"estimateSpaceMinObservations\").evalAsInt())

cmd.append(\"--estimateSpaceMinObservationAngle\")
cmd.append(\"%s\" % node.parent().parm(\"estimateSpaceMinObservationAngle\").evalAsInt())

cmd.append(\"--pixSizeMarginInitCoef\")
cmd.append(\"%s\" % node.parent().parm(\"pixInit\").evalAsFloat())

cmd.append(\"--pixSizeMarginFinalCoef\")
cmd.append(\"%s\" % node.parent().parm(\"pixFinal\").evalAsFloat())

cmd.append(\"--voteMarginFactor\")
cmd.append(\"%s\" % node.parent().parm(\"vote\").evalAsFloat()) 

cmd.append(\"--contributeMarginFactor\")
cmd.append(\"%s\" % node.parent().parm(\"contrib\").evalAsFloat())

cmd.append(\"--simGaussianSizeInit\")
cmd.append(\"%s\" % node.parent().parm(\"simGaussInit\").evalAsFloat())

cmd.append(\"--simGaussianSize\")
cmd.append(\"%s\" % node.parent().parm(\"simGaussSize\").evalAsFloat())

cmd.append(\"--minAngleThreshold\")
cmd.append(\"%s\" % node.parent().parm(\"minThresh\").evalAsFloat())

cmd.append(\"--refineFuse\")
cmd.append(\"%s\" % node.parent().parm(\"rFuse\").evalAsInt())

cmd.append(\"--saveRawDensePointCloud\")
cmd.append(\"%s\" % node.parent().parm(\"saveRawDensePointCloud\").evalAsInt())

cmd.append(\"--verboseLevel\")
cmd.append(\"error\")

hou.pwd().parent().hdaModule().log(cmd, cache, \"09_Meshing\", node)"	)
}

