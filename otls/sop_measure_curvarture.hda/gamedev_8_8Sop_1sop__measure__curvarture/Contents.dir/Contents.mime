vector2 eigvec2 = normalize(set(eigvec1.y, -eigvec1.x));
float k1 = (matrix2(set(eigvec1 * mat, 0)) * transpose(matrix2(set(eigvec1, 0)))).xx;
float k2 = (matrix2(set(eigvec2 * mat, 0)) * transpose(matrix2(set(eigvec2, 0)))).xx;

// compute curvatures
