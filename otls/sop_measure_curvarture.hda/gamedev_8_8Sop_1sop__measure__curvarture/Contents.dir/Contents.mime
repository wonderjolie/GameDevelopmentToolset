vector normal = normalize(prim_normal(@OpInput1, @primnum, 0.333, 0.333));

vector deriv_array[];
resize(deriv_array, 3);

// compute derivatives
foreach(int i; int point; points)
{
    vector edge = point(@OpInput1, \"P\", point) - point(@OpInput1, \"P\", points[i - 1]);
    for(int n = 0; n < 3; n++)
        deriv_array[n] += vector(point(@OpInput1, \"N\", points[(i + 1) % 3]))[n] * cross(normal, edge);
}

// normalize by area
float area = primintrinsic(@OpInput1, \"measuredarea\", @primnum);
for(int i = 0; i < 3; i++)
    deriv_array[i] /= 2 * area; 

v@dx = deriv_array[0];
v@dy = deriv_array[1];
v@dz = deriv_array[2];"	)
