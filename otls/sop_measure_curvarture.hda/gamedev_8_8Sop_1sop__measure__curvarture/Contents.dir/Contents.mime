// get n-ring neighbours
if(nprimitives(@OpInput1))
{
    neighbours = neighbours(@OpInput1, @ptnum);
    int point_array[] = neighbours;
    int temp_array[];
    int count = max(chi(\"n_ring\") - 1, 0);
    
    while(count--)
    {
        int index = 0;
        foreach(int point; point_array)
        {
            int point_neighbours[] = neighbours(@OpInput1, point);
            for(int i = 0; i < len(point_neighbours); i++)
            {
                int p = point_neighbours[i];
                if(find(neighbours, p) < 0)
                {
                    append(neighbours, p);
                    temp_array[index] = p;
                    index += 1;
                }
            }
        }
        point_array = temp_array;
    }
    removevalue(neighbours, @ptnum);
}

// get closest points
else
