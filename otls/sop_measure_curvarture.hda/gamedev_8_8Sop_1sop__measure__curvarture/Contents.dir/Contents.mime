vector eigenvector(const matrix3 mat_input; const int max_iter; const float tol)
{
        matrix3 mat = mat_input;
        vector vec = 1;
        vector vec_max = normalize(vec * mat);
        vector vec_max_temp;
        
        int i = max_iter;
        while(i--)
        {
            vec_max_temp = vec_max;
            vec_max *= mat;
            vec_max = normalize(vec_max);
            int cond = distance(pow(vec_max, 2), pow(vec_max_temp, 2)) < tol;
            if(cond)
                    break;
        }
        return vec_max;
}




int neighbours[] = neighbours(@OpInput1, @ptnum);
@N = normalize(@N);

float mat_array[];
float area_sum = 0;

foreach(int neighbour; neighbours)
{
    vector pos = point(@OpInput1, \"P\", neighbour);
    vector edge = pos - @P;
    matrix3 mat_p = ident() - outerproduct(@N, @N);
    vector edge_p = normalize(edge * mat_p);
    
    // compute directional curvature along edge
    float curv_d = 2 * dot(edge, @N) / dot(edge, edge);
   
    // compute weights
    float area = 0;
    int prim1 = hedge_prim(@OpInput1, pointhedge(@OpInput1, @ptnum, neighbour));
    int prim2 = hedge_prim(@OpInput1, pointhedge(@OpInput1, neighbour, @ptnum));
    if(prim1 != -1)
        area += primintrinsic(@OpInput1, \"measuredarea\", prim1);
    if(prim2 != -1)
        area += primintrinsic(@OpInput1, \"measuredarea\", prim2);
