--HOUDINIMIMEBOUNDARY0xD3ADD339-0x00000F49-0x56B122C9-0x00000001HOUDINIMIMEBOUNDARY
Content-Disposition: attachment; filename="hdaroot/heightfield_project2/perform_projection.parm"
Content-Type: text/plain

{
version 0.8
folder0	[ 0	locks=0 ]	(	0	0	)
group	[ 8	locks=0 ]	(	""	)
bindeach	[ 0	locks=0 ]	(	"off"	)
snippet	[ 0	locks=0 ]	(	"vector up = {0,1,0};

int hitfarthest = chi('hit_farthest');
float maxdist = ch('max_dist');

vector hitpos;
float hitu, hitv;
int hitprim;

vector raydir = up * maxdist;
if (!hitfarthest)
    raydir *= -1;

hitprim = intersect('opinput:1', @P, raydir, hitpos, hitu, hitv, \"farthest\", 1);
// If we failed to find in the main direction, try again in opposite direction
// but grab the closest.
if (hitprim < 0)
{
    raydir *= -1;
    hitprim = intersect('opinput:1', @P, raydir, hitpos, hitu, hitv, \"farthest\", 0);
}

int Outside = prim(1, \"Outside\", hitprim);
if (hitprim >= 0 && Outside)
{
    //  Successfully updated
    float   dist = dot(hitpos, up);
    
    int     method = chi('combine_method');
    if (method == 0)
        @density = dist;
    else if (method == 1)
        @density += dist;
    else if (method == 2)
        @density *= dist;
    else if (method == 3)
        @density = max(dist, @density);
    else if (method == 4)
        @density = min(dist, @density);
}"	)
exportlist	[ 0	locks=0 ]	(	*	)
vex_strict	[ 0	locks=0 ]	(	"off"	)
autobind	[ 0	locks=0 ]	(	"off"	)
