vex_threadjobsize	[ 0	locks=0 ]	(	1024	)
snippet	[ 0	locks=0 ]	(	"// Declare
i@X = -2;
i@Z = -2;
i@myX = -2;
i@myZ = -2;
i@Outside = 0;
//i@newInstance = 0;

// Analyze X axis of tiling grid
if (@P.x > 1.0){

    // Outside of +X, duplicate to -X
    i@Outside = 1;
    i@X = -1;
    i@myX = 1;
} else if (@P.x < 0) {

    // Outside of -X, duplicate to +X
    i@Outside = 1;
    i@X = 1;
    i@myX = 0;
}

// Analyze Z axis tiling grid
if (@P.z > 1.0){
    
    // Outside of +Z, duplicate to -Z
    i@Outside = 1;
    i@Z = -1;
    i@myZ = 1;
} else if (@P.z < 0) {

    // Outside of -Z, duplicate to +Z
    i@Outside = 1;
    i@Z = 1;
    i@myZ = 0;
}"	)
