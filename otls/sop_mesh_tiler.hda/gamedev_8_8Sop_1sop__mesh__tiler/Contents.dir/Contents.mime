vex_outputmask	[ 0	locks=0 ]	(	*	)
folder1	[ 0	locks=0 ]	(	0	0	)
max_dist	[ 0	locks=0 ]	(	[ max_dist	1000 ] 	)
hit_farthest	[ 0	locks=0 ]	(	[ hit_farthest	1 ] 	)
combine_method	[ 0	locks=0 ]	(	[ combine_method	3 ] 	)
bindprimname1	[ 0	locks=0 ]	(	[ bindprimname1	height ] 	)
bindprim1	[ 0	locks=0 ]	(	0	)
bindname1	[ 0	locks=0 ]	(	density	)
